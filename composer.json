{
    "name": "iamprincesly/claude-ai-laravel",
    "description": "Laravel package for Claude AI",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "claude ai",
        "claude",
        "claude-ai-laravel"
    ],
    "homepage": "https://github.com/iamprincesly/claude-ai-laravel",
    "support": {
        "issues": "https://github.com/iamprincesly/claude-ai-laravel/issues",
        "source": "https://github.com/iamprincesly/claude-ai-laravel"
    },
    "autoload": {
        "psr-4": {
            "Iamprincesly\\ClaudeAI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iamprincesly\\ClaudeAI\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "iamprincesly",
            "email": "iamprincesly@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.9",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.23"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iamprincesly\\ClaudeAI\\ClaudeAIServiceProvider"
            ],
            "aliases": {
                "ClaudeAI": "Iamprincesly\\ClaudeAI\\Facades\\ClaudeAIFacade"
            }
        }
    },
    "minimum-stability": "dev"
}